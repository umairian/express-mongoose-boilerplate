{
  "name": "express-mongoose-boilerplate",
  "version": "0.0.1",
  "description": "Boiler Plate Application of Express with Mongoose",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start-server": "./node_modules/pm2/bin/pm2 start server.js",
    "stop-server": "./node_modules/pm2/bin/pm2 stop server.js",
    "logs": "./node_modules/pm2/bin/pm2 logs",
    "test": "npm run test",
    "lint": "npx eslint .",
    "prettier": "npx prettier -c .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+repo_url"
  },
  "author": "",
  "license": "ISC",
  "homepage": "repo_url#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-bunyan-logger": "^1.3.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^5.7.0",
    "mongoose": "^7.3.2",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
